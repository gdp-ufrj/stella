directories/dtl_directory={
"gustavo": "res://gustavo.dtl"
}

[display]

window/size/viewport_width=480
window/size/viewport_height=270
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg")

[file_customization]

folder_colors={
"res://assets/": "red",
"res://scenes/": "green",
"res://scripts/": "blue"
}

